<?xml version="1.0"?>
<schemalist>
  <schema id="org.freedesktop.ibus.engine.gucharmap"
	  path="/desktop/ibus/engine/gucharmap/">
    <key name="use-system-font" type="b">
      <default>true</default>
      <summary>Use system font to draw characters in the charmap</summary>
      <description>If true, use system font to draw characters in the
      charmap.</description>
    </key>
    <key name="font" type="s">
      <default>'Sans 12'</default>
      <summary>The default font used to draw characters in the charmap</summary>
      <description>The default font used to draw characters in the
      charmap, if "use-system-font" is false.</description>
    </key>
    <key name="number-of-matches" type="i">
      <default>100</default>
      <summary>The maximum number of hits when searching</summary>
      <description>Only lists matches below this value when
      searching.</description>
    </key>
    <key name="select-chapter-shortcut" type="s">
      <default>'Alt+Down'</default>
      <summary>Keyboard shortcut to drop down chapters menu</summary>
      <description></description>
    </key>
    <key name="commit-character-shortcut" type="s">
      <default>'Return'</default>
      <summary>Keyboard shortcut to commit character</summary>
      <description></description>
    </key>
  </schema>
</schemalist>
